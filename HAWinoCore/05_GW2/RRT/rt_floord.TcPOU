  <POU Name="rt_floord" Id="{582eea98-ac82-4188-ace6-40cdc8d75d10}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION rt_floord : LREAL
VAR_INPUT
	u: LREAL;
END_VAR
VAR
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
rt_floord := DINT_TO_LREAL(TRUNC(u));

IF u = rt_floord THEN 
    rt_floord := u;
ELSIF u < 0.0 THEN 
    rt_floord := rt_floord - 1.0;
END_IF;


]]></ST>
    </Implementation>
